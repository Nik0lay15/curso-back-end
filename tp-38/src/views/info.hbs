<div class="container-sm p-3 mb-5 bg-light">
    <h1>Argumentos:</h1>
        {{#each payload.args}}
            <h5>{{this}}</h5>
        {{/each}}   
    <br> 

    <h1>OS</h1>
    <h5>{{payload.os}}</h5>
    <br>

    <h1>Version de node</h1>
    <h5>{{payload.version}}</h5>
    <br>

    <h1>Uso total de memoria</h1>
    <h5>{{payload.memoria}}MB</h5>
    <br>

    <h1>Path de ejecucion</h1>
    <h5>{{payload.path}}</h5>
    <br>

    <h1>ID de proceso</h1>
    <h5>{{payload.pid}}</h5>
    <br>

    <h1>Carpeta raiz</h1>
    <h5>{{payload.root}}</h5>
</div>